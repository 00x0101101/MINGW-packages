+/* Client side decorations */
+.titlebar {
+  text-shadow: 1px 1px lighter (@bg_color);
+  background-image: linear-gradient(to bottom, white, @bg_color);
+  border-radius: 7px 7px 0px 0px;
+}
+
+.maximized .titlebar,
+.tiled .titlebar {
+  border-radius: 0;
+}
+
+.titlebar.default-decoration {
+  border: none;
+  box-shadow: none;
+}
+
+.titlebar .title {
+  font: Bold 10;
+}
+
+.titlebar .subtitle {
+  font: 9;
+}
+
+.titlebar:backdrop {
+  text-shadow: none;
+  background-image: none;
+  background-color: @bg_color;
+}
+
+.titlebar .titlebutton {
+  icon-shadow: 0px 1px #ffff;
+  color: shade(@fg_color, 1.8);
+  background: none;
+
+  border-radius: 3px;
+  border-width: 1px 1px 2px 1px;
+  border-color: transparent;
+  border-style: solid;
+}
+
+.titlebar .titlebutton:hover {
+  background-image: -gtk-win32-theme-part(button, 1 2);
+}
+.titlebar .titlebutton:active {
+  background-image: -gtk-win32-theme-part(button, 1 3);
+  color: @selected_fg_color;
+  icon-shadow: none;
+}
+
+.titlebar .titlebutton:backdrop {
+  background-image: none;
+  color: #a7aba7;
+  border-image: none;
+  icon-shadow: none;
+}
+
+.window-frame {
+  border-color: darker (@bg_color);
+  border-radius: 7px 7px 0 0;
+  border-width: 1px;
+  border-style: solid;
+
+  box-shadow: 0 2px 7px 3px alpha(black, 0.6);
+  margin: 10px;
+}
+
+.window-frame.tiled {
+  border-radius: 0;
+  background-color: @bg_color;
+}
+
+.window-frame:backdrop {
+  box-shadow: 0 2px 5px 1px alpha(black, 0.4);
+}
